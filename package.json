{
  "name": "wurd",
  "scripts": {
    "analyze": "npm run build -s -- --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build": "NODE_ENV=production webpack -p",
    "dev": "NODE_ENV=development nodemon --watch server server/server.js & webpack-dev-server --hot",
    "format": "prettier --write \"{,__mocks__/**/,client/**/,server/**/}*.{js,json,scss}\"",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . && stylelint \"client/**/*.scss\"",
    "start": "node server/server.js",
    "storybook": "start-storybook -p 9001 -c client/.storybook",
    "test": "jest"
  },
