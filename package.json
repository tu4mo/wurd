{
  "name": "wurd",
  "scripts": {
    "analyze": "npm run build -s -- --profile --json > stats.json && webpack-bundle-analyzer stats.json dist",
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development nodemon --watch server server/server.js & webpack-dev-server --hot",
    "format": "prettier --write \"**/*.{js,json,scss}\"",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . && stylelint \"client/**/*.scss\" && stylelint \"client/**/*.js\" --config stylelint.js.config.js",
    "start": "node server/server.js",
    "storybook": "start-storybook -p 9001 -c client/.storybook",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "jest --coverage",
    "test:e2e": "testcafe -e chrome test",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "9.9.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^1.0.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1",
    "md5": "^2.2.1",
    "moment": "^2.22.0",
    "mongoose": "^5.0.13",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-container-query": "^0.11.0",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.1",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-navigation-prompt": "^1.6.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-options": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
