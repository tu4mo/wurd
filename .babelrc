{
  "presets": [
    [
      "env", { "modules": false }
    ],
    "react"
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "client"
      }
    ],
    "syntax-dynamic-import",
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react"
      ]
    }
  }
}
